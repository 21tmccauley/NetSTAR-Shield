{
  
    "source": {
    "include": ["../"], 
    "includePattern": ".+\\.(js(doc|x)?|mjs)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "Extension/docs/jsdocs",
    "recurse": true,
    "encoding": "utf8"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
